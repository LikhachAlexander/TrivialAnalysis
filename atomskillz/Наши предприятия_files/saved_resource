			$('form.search-form input.speciality').autocomplete({minLength:0,source:[]}).click(function(){
				$(this).autocomplete('search',this.value);
			});

			$('form.search-form input.degree').change(function(){
				var $this=$(this),
					$form=$this.parents('form.search-form');
				if($form.length!=1) return;
				var form=$form[0],
					degree=form.degree.value,
					autocomplete=specialties[degree];
				if(autocomplete==undefined) autocomplete=[];
				$form.find('input.speciality').autocomplete('option','source',autocomplete);
			}).change();
		});
	</script>
