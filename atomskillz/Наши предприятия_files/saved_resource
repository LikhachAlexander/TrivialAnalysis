<script>
var res;
var now = new Date();
switch(now.getMonth())
{
case 0:
    res="<span>Январь</span>";
    break;
case 1:
    res="<span>Февраль</span>";
    break;
case 2:
    res="<span>Март</span>";
    break;
case 3:
    res="<span>Апрель</span>";
    break;
case 4:
    res="<span>Май</span>";
    break;
case 5:
    res="<span>Июнь</span>";
    break;
case 6:
    res="<span>Июль</span>";
    break;
case 7:
    res="<span>Август</span>";
    break;
case 8:
    res="<span>Сентябрь</span>";
    break;
case 9:
    res="<span>Октябрь</span>";
    break;
case 10:
    res="<span>Ноябрь</span>";
    break;
case 11:
    res="<span>Декабрь</span>";
    break;
}
$(document).ready(function(){
 $(".month").html(res);
}); 
</script>
